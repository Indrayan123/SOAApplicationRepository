      </copy>
      <copy>
        <from expression="snidttm:convertXSDDttmToFormattedDttm(&quot;2010-10-13T15:16:00.000Z&quot;,&quot;US/Central&quot;,&quot;yyyy-MM-dd'T'HH:mm:ss&quot;)"/>
        <to variable="temp2"/>
      </copy>
      <copy>
        <from expression="'success'"/>
        <to variable="outputVariable" part="payload"
            query="/client:processResponse/client:result"/>
      </copy>
    </assign>
    <reply name="replyOutput" partnerLink="fndintgdatexpathivp_client" portType="client:FndIntgDateXpathIVP" operation="process" variable="outputVariable"/>
