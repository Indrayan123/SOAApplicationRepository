                        <from expression="'Failure'"/>
                        <to variable="outputVariable" part="part"
                            query="/ns10:Output/ns10:ValidateServiceIntRes/ns10:fndintgumsmailprocessivp/ns10:Status"/>
                      </copy>
                      <copy>
                        <from expression="'UMS Mail Consumption did not Work'"/>
