  Oracle JDeveloper BPEL Designer 
  
  Created: Wed Sep 29 15:19:48 CDT 2010
  Author:  c88525
  Purpose: Synchronous BPEL Process
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="BPELProcess1"
               targetNamespace="http://xmlns.oracle.com/SNIFndIntgIVPServices_jws/FndIntg_SimpleBRIVP/BPELProcess1"
               xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
               xmlns:client="http://xmlns.oracle.com/SNIFndIntgIVPServices_jws/FndIntg_SimpleBRIVP/BPELProcess1"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
               xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns1="http://xmlns.oracle.com/BR_simpulerule/BR_simpulerule_DecisionService_1"
         xmlns:ns2="http://siebel.com/OrderManagement/Order/Data"
         xmlns:ns3="http://xmlns.oracle.com/bpel"
         xmlns:ns13="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIADFExtensionFunctions"
         xmlns:ns6="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns14="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns8="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:ns16="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:ns17="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIOptimisticLockExtensionFunctions"
         xmlns:ns10="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIDateTimeExtensionFunctions"
         xmlns:ns18="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:ns15="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ns12="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIFormatExtensionFunctions"
         xmlns:ns5="http://xmlns.schneider.com/BPEL/sni/foundation/soa/extensions/xpath"
         xmlns:ns11="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIPropertyFileExtensionFunctions"
         xmlns:ns4="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIDVMExtensionFunctions"
         xmlns:ns9="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.utils.SNIUtilityExtensionFunctions"
         xmlns:ns19="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIXRefExtensionFunctions"
         xmlns:ns7="http://schemas.oracle.com/xpath/extension/ldap">

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="bpelprocess1_client" partnerLinkType="client:BPELProcess1" myRole="BPELProcess1Provider"/>
    <partnerLink name="BR_Service" partnerRole="IDecisionService"
                 partnerLinkType="ns1:BR_Service"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:BPELProcess1RequestMessage"/>

    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable" messageType="client:BPELProcess1ResponseMessage"/>
    <variable name="Invoke_1_callFunctionStateful_InputVariable"
              messageType="ns1:callFunctionStatefulMessage"/>
    <variable name="Invoke_1_callFunctionStateful_OutputVariable"
              messageType="ns1:callFunctionStatefulDecisionMessage"/>
  </variables>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in BPELProcess1.wsdl) -->
    <receive name="receiveInput" partnerLink="bpelprocess1_client" portType="client:BPELProcess1" operation="process" variable="inputVariable" createInstance="yes"/>

    <!-- Generate reply to synchronous request -->
    <assign name="Assign_1">
      <copy>
        <from expression="'BR_simpulerule_DecisionService_1'"/>
        <to variable="Invoke_1_callFunctionStateful_InputVariable"
            part="payload" query="/ns1:callFunctionStateful/@name"/>
      </copy>
      <copy>
        <from expression="ora:getProcessVersion()"/>
        <to variable="Invoke_1_callFunctionStateful_InputVariable"
            part="payload"
            query="/ns1:callFunctionStateful/ns1:bpelInstance/ns3:processVersion"/>
      </copy>
      <copy>
        <from expression="'soa4'"/>
        <to variable="Invoke_1_callFunctionStateful_InputVariable"
            part="payload"
            query="/ns1:callFunctionStateful/ns1:bpelInstance/ns3:domainId"/>
      </copy>
      <copy>
        <from expression="ora:getProcessURL()"/>
        <to variable="Invoke_1_callFunctionStateful_InputVariable"
            part="payload"
            query="/ns1:callFunctionStateful/ns1:bpelInstance/ns3:processURL"/>
      </copy>
      <copy>
        <from expression="ora:getInstanceId()"/>
        <to variable="Invoke_1_callFunctionStateful_InputVariable"
            part="payload"
            query="/ns1:callFunctionStateful/ns1:bpelInstance/ns3:instanceId"/>
      </copy>
      <copy>
        <from expression="'ConvID'"/>
        <to variable="Invoke_1_callFunctionStateful_InputVariable"
            part="payload"
            query="/ns1:callFunctionStateful/ns1:bpelInstance/ns3:conversationId"/>
      </copy>
      <copy>
        <from expression="ora:getProcessId()"/>
        <to variable="Invoke_1_callFunctionStateful_InputVariable"
            part="payload"
            query="/ns1:callFunctionStateful/ns1:bpelInstance/ns3:processId"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns2:SWIOrderOutputs/ns2:Value1"/>
        <to variable="Invoke_1_callFunctionStateful_InputVariable"
            part="payload"
            query="/ns1:callFunctionStateful/ns1:parameterList/ns2:SWIOrderOutputs/ns2:Value1"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns2:SWIOrderOutputs/ns2:Value2"/>
        <to variable="Invoke_1_callFunctionStateful_InputVariable"
            part="payload"
            query="/ns1:callFunctionStateful/ns1:parameterList/ns2:SWIOrderOutputs/ns2:Value2"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns2:SWIOrderOutputs/ns2:Value3"/>
        <to variable="Invoke_1_callFunctionStateful_InputVariable"
            part="payload"
            query="/ns1:callFunctionStateful/ns1:parameterList/ns2:SWIOrderOutputs/ns2:Value3"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns2:SWIOrderOutputs/ns2:Value4"/>
        <to variable="Invoke_1_callFunctionStateful_InputVariable"
            part="payload"
            query="/ns1:callFunctionStateful/ns1:parameterList/ns2:SWIOrderOutputs/ns2:Value4"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns2:SWIOrderOutputs/ns2:Value5"/>
        <to variable="Invoke_1_callFunctionStateful_InputVariable"
            part="payload"
            query="/ns1:callFunctionStateful/ns1:parameterList/ns2:SWIOrderOutputs/ns2:Value5"/>
      </copy>
      <copy>
        <from variable="inputVariable" part="payload"
              query="/ns2:SWIOrderOutputs/ns2:Value6"/>
        <to variable="Invoke_1_callFunctionStateful_InputVariable"
            part="payload"
            query="/ns1:callFunctionStateful/ns1:parameterList/ns2:SWIOrderOutputs/ns2:Value6"/>
      </copy>
    </assign>
    <invoke name="Invoke_1"
            inputVariable="Invoke_1_callFunctionStateful_InputVariable"
            outputVariable="Invoke_1_callFunctionStateful_OutputVariable"
            partnerLink="BR_Service" portType="ns1:IDecisionService"
            operation="callFunctionStateful"/>
    <assign name="Assign_2">
      <copy>
        <from variable="Invoke_1_callFunctionStateful_OutputVariable"
              part="payload"
              query="/ns1:callFunctionStatefulDecision/ns1:resultList/ns2:SWIOrderOutputs/ns2:Value2"/>
        <to variable="outputVariable" part="payload"
            query="/ns2:SWIOrderOutputs/ns2:Value2"/>
      </copy>
    </assign>
    <reply name="replyOutput" partnerLink="bpelprocess1_client" portType="client:BPELProcess1" operation="process" variable="outputVariable"/>
  </sequence>
</process>