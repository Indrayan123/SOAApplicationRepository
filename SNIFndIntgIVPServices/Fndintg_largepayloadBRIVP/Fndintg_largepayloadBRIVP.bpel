<process name="Fndintg_largepayloadBRIVP"
         targetNamespace="http://xmlns.oracle.com/SNIFndIntgIVPServices_jws/Fndintg_largepayloadBRIVP/Fndintg_largepayloadBRIVP"
         xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:client="http://xmlns.oracle.com/SNIFndIntgIVPServices_jws/Fndintg_largepayloadBRIVP/Fndintg_largepayloadBRIVP"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns1="http://xmlns.oracle.com/BR_largepayload_rule/BR_largepayload_rule_DecisionService_1"
         xmlns:ns4="http://xmlns.schneider.com/EnterpriseObjects/Core/Common/V1"
         xmlns:ns2="http://xmlns.oracle.com/bpel"
         xmlns:ns3="http://siebel.com/OrderManagement/Order/Data"
         xmlns:ns14="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIADFExtensionFunctions"
         xmlns:ns7="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ns15="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns9="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:ns17="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:ns18="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIOptimisticLockExtensionFunctions"
         xmlns:ns11="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIDateTimeExtensionFunctions"
         xmlns:ns19="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:ns16="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:ns13="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIFormatExtensionFunctions"
         xmlns:ns6="http://xmlns.schneider.com/BPEL/sni/foundation/soa/extensions/xpath"
         xmlns:ns12="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIPropertyFileExtensionFunctions"
         xmlns:ns10="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.utils.SNIUtilityExtensionFunctions"
         xmlns:ns5="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIDVMExtensionFunctions"
         xmlns:ns20="http://www.oracle.com/XSL/Transform/java/sni.foundation.soa.extensions.xpath.SNIXRefExtensionFunctions"
         xmlns:ns8="http://schemas.oracle.com/xpath/extension/ldap">

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
    <partnerLinks>
        <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
        <partnerLink name="LargepayloadBR" partnerRole="IDecisionService"
                     partnerLinkType="ns1:LargepayloadBR"/>
        <partnerLink name="fndintg_largepayloadbrivp_client_ep"
                     partnerLinkType="client:Fndintg_largepayloadBRIVP"
                     myRole="Fndintg_largepayloadBRIVPProvider"/>
    </partnerLinks>

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        VARIABLES                                                        
        List of messages and XML documents used within this BPEL process 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <variables>
        <!-- Reference to the message passed as input during initiation -->
        <variable name="inputVariable" messageType="client:Fndintg_largepayloadBRIVPRequestMessage"/>

        <!-- Reference to the message that will be sent back to the requester during callback -->
        <variable name="outputVariable" messageType="client:Fndintg_largepayloadBRIVPResponseMessage"/>
        <variable name="Invoke_1_callFunctionStateless_InputVariable"
                  messageType="ns1:callFunctionStatelessMessage"/>
        <variable name="Invoke_1_callFunctionStateless_OutputVariable"
                  messageType="ns1:callFunctionStatelessDecisionMessage"/>
    </variables>

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
       ORCHESTRATION LOGIC                                               
       Set of activities coordinating the flow of messages across the    
       services integrated within this business process                  
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <sequence name="main">
        <!-- Receive input from requestor. (Note: This maps to operation defined in Fndintg_largepayloadBRIVP.wsdl) -->
        <receive name="receiveInput"
                 partnerLink="fndintg_largepayloadbrivp_client_ep" portType="client:Fndintg_largepayloadBRIVP" operation="process" variable="inputVariable" createInstance="yes"/>

        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
        <assign name="Assign_1">
            <copy>
                <from expression="'BR_largepayload_rule_DecisionService_1'"/>
                <to variable="Invoke_1_callFunctionStateless_InputVariable"
                    part="payload" query="/ns1:callFunctionStateless/@name"/>
            </copy>
            <copy>
                <from expression="ora:getProcessId()"/>
                <to variable="Invoke_1_callFunctionStateless_InputVariable"
                    part="payload"
                    query="/ns1:callFunctionStateless/ns1:bpelInstance/ns2:processId"/>
            </copy>
            <copy>
                <from expression="ora:getProcessVersion()"/>
                <to variable="Invoke_1_callFunctionStateless_InputVariable"
                    part="payload"
                    query="/ns1:callFunctionStateless/ns1:bpelInstance/ns2:processVersion"/>
            </copy>
            <copy>
                <from expression="'soadomain'"/>
                <to variable="Invoke_1_callFunctionStateless_InputVariable"
                    part="payload"
                    query="/ns1:callFunctionStateless/ns1:bpelInstance/ns2:domainId"/>
            </copy>
            <copy>
                <from expression="ora:getProcessURL()"/>
                <to variable="Invoke_1_callFunctionStateless_InputVariable"
                    part="payload"
                    query="/ns1:callFunctionStateless/ns1:bpelInstance/ns2:processURL"/>
            </copy>
            <copy>
                <from expression="ora:getInstanceId()"/>
                <to variable="Invoke_1_callFunctionStateless_InputVariable"
                    part="payload"
                    query="/ns1:callFunctionStateless/ns1:bpelInstance/ns2:instanceId"/>
            </copy>
            <copy>
                <from expression="'ConvID'"/>
                <to variable="Invoke_1_callFunctionStateless_InputVariable"
                    part="payload"
                    query="/ns1:callFunctionStateless/ns1:bpelInstance/ns2:conversationId"/>
            </copy>
            <copy>
                <from variable="inputVariable" part="payload"
                      query="/ns3:SWIOrderIO/ns3:SWIOrder/ns3:SNIAccountId"/>
                <to variable="Invoke_1_callFunctionStateless_InputVariable"
                    part="payload"
                    query="/ns1:callFunctionStateless/ns1:parameterList/ns3:SWIOrderIO/ns3:SWIOrder/ns3:SNIAccountId"/>
            </copy>
        </assign>
        <invoke name="Invoke_1"
                inputVariable="Invoke_1_callFunctionStateless_InputVariable"
                outputVariable="Invoke_1_callFunctionStateless_OutputVariable"
                partnerLink="LargepayloadBR" portType="ns1:IDecisionService"
                operation="callFunctionStateless"/>
        <assign name="Assign_2">
            <copy>
                <from variable="Invoke_1_callFunctionStateless_OutputVariable"
                      part="payload"
                      query="/ns1:callFunctionStatelessDecision/ns1:resultList/ns3:SWIOrderOutputs/ns3:Value1"/>
                <to variable="outputVariable" part="payload"
                    query="/ns3:SWIOrderOutputs/ns3:Value1"/>
            </copy>
        </assign>
        <reply name="Replyoutput" variable="outputVariable"
               partnerLink="fndintg_largepayloadbrivp_client_ep"
               portType="client:Fndintg_largepayloadBRIVP" operation="process"/>
    </sequence>
</process>