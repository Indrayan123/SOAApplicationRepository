      <copy bpelx:insertMissingToData="yes" bpelx:ignoreMissingFromData="yes">
        <from expression="bpws:getVariableData('ExecutionStartTime')"/>
        <to variable="ValidateALLSOAIVPResMsg" part="part"
            query="/ns9:Output/ns9:OtherDetails/ns9:ExecutionStartTime"/>
      </copy>
      <copy bpelx:ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
        <from expression="bpws:getVariableData('ExecutionEndTime')"/>
        <to variable="ValidateALLSOAIVPResMsg" part="part"
            query="/ns9:Output/ns9:OtherDetails/ns9:ExecutionEndTime"/>
      </copy>
      <copy bpelx:ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
        <from expression="sniutil:getHostName()"/>
        <to variable="ValidateALLSOAIVPResMsg" part="part"
            query="/ns9:Output/ns9:OtherDetails/ns9:ServerName"/>
      </copy>
    </assign>
    <assign name="Xform_TrimEmptyElements_Output">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:doXSLTransformForDoc('xsl/Xform_TrimEmptyElements_Output.xsl', $ValidateALLSOAIVPResMsg.part)"/>
        <to variable="ValidateALLSOAIVPResMsg" part="part"/>
      </copy>
    </assign>
    <!-- Generate reply to synchronous request -->
    <reply name="replyOutput" partnerLink="FndIntgValidateALLSOAIVPsvc"
           portType="ns1:ValidateALLSOAIVP_ptt" operation="ValidateALLSOAIVP"
           variable="ValidateALLSOAIVPResMsg"/>
    <assign name="AssignFileWriteReqMsg">
      <copy bpelx:insertMissingToData="yes" bpelx:ignoreMissingFromData="yes">
        <from variable="ValidateALLSOAIVPResMsg" part="part"
              query="/ns9:Output"/>
        <to variable="ValidateALLSOAIVPFileWriteReqMsg" part="body"
            query="/ns17:Output"/>
      </copy>
      <copy bpelx:ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
        <from expression='ora:getPreference("FileWriteDirectory")'/>
        <to variable="ValidateALLFileWritePath"/>
      </copy>
      <copy>
        <from expression="concat('ValidateALLSOAIVPOutput_',ora:getPreference('FileName'),'.xml')"/>
        <to variable="Filename"/>
      </copy>
      <copy>
        <from expression="snibpel:logSNIMessage(bpws:getVariableData('SNIHeader'),concat('FndIntgValidateALLSOAIVP-Exit',bpws:getVariableData('SNIHeader','/ns20:SNIHeader/ns20:BusinessKey')),string('INFO'))"/>
        <to variable="logStatus"/>
      </copy>
    </assign>
    <invoke name="InvokeValidateALLSOAIVPFileWrite"
            inputVariable="ValidateALLSOAIVPFileWriteReqMsg"
            partnerLink="FndIntgValidateALLSOAIVPFileWriteService"
            portType="ns15:Write_ptt" operation="Write"
            bpelx:invokeAsDetail="no">
      <bpelx:inputProperty name="jca.file.FileName" variable="Filename"/>
      <bpelx:inputProperty name="jca.file.Directory"
                           variable="ValidateALLFileWritePath"/>
    </invoke>
  </sequence>
</process>