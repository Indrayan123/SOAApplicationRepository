      <copy>
        <from expression="snibpel:EnterpriseCachePutByCacheName('FNDCache-SOAIVPStatus',bpws:getVariableData('keyIdentifier'), 'Task Created', '172800000')"/>
        <to variable="cacheStatus"/>
      </copy>
    </assign>
