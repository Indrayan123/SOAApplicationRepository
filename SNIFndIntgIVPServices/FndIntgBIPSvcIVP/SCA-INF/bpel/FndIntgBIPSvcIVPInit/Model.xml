          <copy>
            <from expression="'Timeout value crossed, process not completed yet'"/>
            <to variable="outputVariable" part="payload" query="/ns3:processResponse/ns3:additionalInfo"/>
          </copy>
        </assign>
      </case>
      <case condition="bpws:getVariableData('cacheStatus')='Task Completed'">
        <assign name="Assign_Status" id="BpAss8">
