      <copy>
        <from expression="xref:populateXRefRow(string(bpws:getVariableData('inputVariable','payload','/client:process/client:xrefLocation')),string(bpws:getVariableData('inputVariable','payload','/client:process/client:refColumnname')),string(bpws:getVariableData('inputVariable','payload','/client:process/client:refValue')),string(bpws:getVariableData('inputVariable','payload','/client:process/client:columnName')),string(bpws:getVariableData('inputVariable','payload','/client:process/client:Value')),string(bpws:getVariableData('inputVariable','payload','/client:process/client:Mode')))"/>
